<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.laboratory_project.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label contentDisplay="TEXT_ONLY" graphicTextGap="11.0" prefHeight="190.0" prefWidth="616.0" text="Система учета данных для сотрудников Лаборатории" textAlignment="CENTER" wrapText="true">
      <font>
         <Font name="Arial" size="30.0" />
      </font></Label>
   <TextField fx:id="TextBox1" prefHeight="36.0" prefWidth="550.0" promptText="Введите пароль для входа" />
    <Button fx:id="Button1" onAction="#Button1" prefHeight="36.0" prefWidth="115.0" text="Войти" />

    <Label fx:id="welcomeText" prefHeight="41.0" prefWidth="102.0" textAlignment="CENTER" wrapText="true" />
</VBox>
